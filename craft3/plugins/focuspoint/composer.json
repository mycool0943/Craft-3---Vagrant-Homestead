        "craftcms",
        "craft-plugin",
        "focuspoint"
    ],
    "support": {
        "docs": "https://github.com/roundhouse/focuspoint/blob/master/README.md",
        "issues": "https://github.com/roundhouse/focuspoint/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Goncharov",
